# This file contain environment variables that affect how balerd.master.service
# starts baler daemon.

# store path
BALERD_STORE=@sharedstatedir@/baler/store

# store plugin
BALERD_STORE_PLUGIN=bstore_sos

# store plugin path
BSTORE_PLUGIN_PATH=@libdir@

# Plugin configuration file
BALERD_PLUGIN_CONFIG=@sysconfdir@/baler/balerd.conf

# Log level
BALERD_LOG_LEVEL=WARN

# The number of input queue workers
BALERD_IN_Q_WORKERS=1

# The number of output queue workers
BALERD_OUT_Q_WORKERS=1
