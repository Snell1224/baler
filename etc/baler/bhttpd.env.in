# This file contain environment variables that affect how bhttpd.service
# starts baler daemon.

# path to the store created by balerd (slave or master)
BHTTPD_STORE=@sharedstatedir@/baler/master.store

# HTTP port
BHTTPD_PORT=10080

# Log level
BHTTPD_LOG_LEVEL=WARN

# Number of task processing threads
BHTTPD_WORKERS=1

# The following options are configured by automake, please don't change them.
ZAP_LIBPATH=@ZAP_LIBPATH@
